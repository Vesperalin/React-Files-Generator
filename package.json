{
  "name": "react-files-generator",
  "displayName": "React Files Generator",
  "description": "",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:react-files-generator.createFolderWithFiles",
    "onCommand:react-files-generator.test"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "when": "explorerResourceIsFolder",
        "command": "react-files-generator.createFolderWithFiles",
        "group": "bookmarks",
        "title": "Create files"
      },
      {
        "command": "react-files-generator.test",
        "group": "bookmarks",
        "title": "Test"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "react-files-generator.createFullSetOfFiles",
          "group": "bookmarks",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "react-files-generator.test",
          "group": "bookmarks"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}
