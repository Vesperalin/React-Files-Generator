{
	"name": "react-files-generator",
	"displayName": "React Files Generator",
	"description": "",
	"version": "0.0.1",
	"publisher": "Klaudia Błażyczek",
	"engines": {
		"vscode": "^1.80.0"
	},
	"vsce": {
		"baseImagesUrl": "https://github.com/Vesperalin/React-Files-Generator/tree/images",
		"dependencies": true,
		"yarn": false
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/Vesperalin/React-Files-Generator.git"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:react-files-generator.createFolderWithFiles"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"when": "explorerResourceIsFolder",
				"command": "react-files-generator.createFolderWithFiles",
				"group": "bookmarks",
				"title": "Create files"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "react-files-generator.createFolderWithFiles",
					"group": "bookmarks",
					"when": "explorerResourceIsFolder"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "tsc --noEmit && eslint src/**/*.ts --cache --max-warnings=0",
		"lint:fix": "eslint src/**/*.ts --fix",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "20.2.5",
		"@types/vscode": "^1.80.0",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"eslint": "^8.45.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^5.0.0",
		"ts-loader": "^9.4.4",
		"typescript": "^5.1.6",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@vscode/test-electron": "^2.3.3",
		"eslint-import-resolver-webpack": "^0.13.2",
		"fs": "^0.0.1-security",
		"glob": "^8.1.0",
		"mocha": "^10.2.0"
	}
}
